{
  "name": "trump-scanner",
  "version": "1.0.0",
  "description": "A comprehensive system for scraping, analyzing, and storing Trump-related legal documents and quotes",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "build": "prisma generate && tsc",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "setup": "node scripts/setup.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "backup": "node scripts/backup.js",
    "restore": "node scripts/restore.js",
    "cleanup": "node scripts/cleanup.js",
    "scrape": "node src/scripts/start_scraping.js",
    "verify": "node src/scripts/verify_documents.js"
  },
  "keywords": [
    "trump",
    "legal",
    "scraper",
    "analysis",
    "database",
    "monitoring"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "axios": "^1.6.7",
    "bull": "^4.12.2",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.1",
    "puppeteer": "^22.1.0",
    "sharp": "^0.33.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "prisma": "^5.10.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/trump-scanner.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/trump-scanner/issues"
  },
  "homepage": "https://github.com/yourusername/trump-scanner#readme"
}
